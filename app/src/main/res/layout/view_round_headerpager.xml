<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.pet.travel.view.imageview.RoundAngleImageView
        android:layout_width="match_parent"
        android:layout_height="@dimen/margin_180"
        android:scaleType="centerCrop"
        android:src="@mipmap/home_item_bg"
        tools:roundHeight="@dimen/margin_15"
        tools:roundWidth="@dimen/margin_15"
        tools:roundLeftBottom="false"
        tools:roundRightBottom="false">

    </com.pet.travel.view.imageview.RoundAngleImageView>

    <android.support.v4.view.ViewPager
        android:id="@+id/viewpager"
        android:layout_width="match_parent"
        android:layout_height="@dimen/margin_180">

    </android.support.v4.view.ViewPager>
</RelativeLayout>
